FUNCTION $AlumatToolsForDrilling(STRING $TOOLNUMBER , OUTPUT STRING $NAME , OUTPUT INT $SPEED , OUTPUT INT $FEED , OUTPUT INT $PLUNGE) {
	
SWITCH $TOOLNUMBER {
	CASE "T232"
		$NAME =  "T232 Punta D11,9 H60,0"
		$SPEED = 2150
		$FEED = 140
		$PLUNGE = 140
	BREAK
	CASE "T806"
		$NAME =  "T806 Conica Toroidale D15,0 H65,0"
		$SPEED = 5000
		$FEED = 1000
		$PLUNGE = 1000
	BREAK
	DEFAULT
	BREAK
}

}




